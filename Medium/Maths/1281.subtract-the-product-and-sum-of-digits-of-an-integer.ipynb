    "class Solution:\n",
    "    def subtractProductAndSum(self, n: int) -> int:\n",
    "        n=str(n)\n",
    "        s,p=0,1\n",
    "        for d in n:\n",
    "            s+=int(d)\n",
    "            p*=int(d)\n",
    "        return p-s"
   ]
