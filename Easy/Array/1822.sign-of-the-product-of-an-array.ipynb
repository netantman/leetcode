    "class Solution:\n",
    "    def arraySign(self, nums: List[int]) -> int:\n",
    "        res = 1\n",
    "        for n in nums:\n",
    "            if n == 0:\n",
    "                return 0\n",
    "            res *= 1 if n > 0 else -1\n",
    "        return res"
   ]
