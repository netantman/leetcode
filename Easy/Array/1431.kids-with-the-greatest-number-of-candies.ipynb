    "class Solution:\n",
    "    def kidsWithCandies(self, candies: List[int], extraCandies: int) -> List[bool]:\n",
    "        m = max(candies)\n",
    "        res = [ c+extraCandies>=m for c in candies ]\n",
    "        return res"
