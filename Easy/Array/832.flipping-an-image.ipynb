    "class Solution:\n",
    "    def flipAndInvertImage(self, A: List[List[int]]) -> List[List[int]]:\n",
    "        new=[a[::-1] for a in A]\n",
    "        new=[[1-elem for elem in n] for n in new]\n",
    "        return new"
