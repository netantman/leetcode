   "source": [
    "class Solution:\n",
    "    def largestAltitude(self, gain: List[int]) -> int:\n",
    "        cumsum = [0]\n",
    "        for g in gain:\n",
    "            cumsum.append(cumsum[-1] + g)\n",
    "        \n",
    "        return max(cumsum)"
   ]
