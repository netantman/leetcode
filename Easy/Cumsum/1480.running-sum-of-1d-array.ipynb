    "class Solution:\n",
    "    def runningSum(self, nums: List[int]) -> List[int]:\n",
    "        cumsum = [0]\n",
    "        for n in nums:\n",
    "            cumsum.append(n + cumsum[-1])\n",
    "        return cumsum[1:]"
   ]
