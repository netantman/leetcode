    "class Solution:\n",
    "    def decode(self, encoded: List[int], first: int) -> List[int]:\n",
    "        res = [first]\n",
    "        for e in encoded:\n",
    "            res.append(res[-1] ^ e)\n",
    "        return res"
   ]
