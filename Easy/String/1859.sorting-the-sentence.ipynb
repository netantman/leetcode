    "class Solution:\n",
    "    def sortSentence(self, s: str) -> str:\n",
    "        s = s.split(\" \")\n",
    "        s = sorted(s, key=lambda x: x[-1])\n",
    "        s = [x[:-1] for x in s]\n",
    "        return \" \".join(s)"
