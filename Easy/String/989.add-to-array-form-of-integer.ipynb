    "class Solution:\n",
    "    def addToArrayForm(self, A: List[int], K: int) -> List[int]:\n",
    "        A=int(\"\".join([str(a) for a in A]))\n",
    "        res=[int(c) for c in str(A+K)]\n",
    "        return res"
