   "source": [
    "class Solution:\n",
    "    def areOccurrencesEqual(self, s: str) -> bool:\n",
    "        record = dict()\n",
    "        for c in s:\n",
    "            record[c] = record.get(c, 0) + 1\n",
    "        return len(set(record.values())) == 1"
